//call the variables and strings from inputboxes and buttons
//used at statr of program


with obj_textbox
{
    switch(tbx_name)
    {
        case "tbn_name": ipv_name = tbx_text;       break;
        case "tbn_year": ipv_year = tbx_text;       break;
        case "tbn_month": ipv_month = tbx_text;       break;
        case "tbn_day": ipv_day = tbx_text;       break;
        case "tbn_time": ipv_time = tbx_text;       break;
    }
}
